{
  "name": "@j-junckes/prisma-generator-nestjs-dto",
  "description": "Generates DTO and Entity classes from Prisma Schema for NestJS",
  "version": "1.6.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Iris Junckes",
    "email": "me@junckes.dev"
  },
  "main": "dist/index.js",
  "keywords": [
    "prisma",
    "generator",
    "nestjs",
    "dto",
    "typescript"
  ],
  "scripts": {
    "prebuild": "npm run cleanup:generated",
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\"",
    "prelint": "npm run format -- --loglevel error",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\" --fix",
    "pretest": "npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "cleanup:generated": "rimraf src/@generated",
    "pregenerate": "npm run cleanup:generated",
    "generate": "npx prisma generate && npm run format -- --loglevel error"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/j-junckes/prisma-generator-nestjs-dto"
  },
  "bin": {
    "prisma-generator-nestjs-dto": "dist/cli.js"
  },
  "engines": {
    "node": ">=14.18"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/generator-helper": "^5.7.1",
    "@prisma/sdk": "^3.15.2",
    "case": "^1.6.3",
    "make-dir": "^4.0.0",
    "slash": "^5.1.0",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "prettier": "^3.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  }
}
